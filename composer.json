{
  "name": "micronative/mock-server",
  "description": "A mock server for client-server testing purpose",
  "keywords": [
    "Mock server",
    "Client server testing"
  ],
  "homepage": "https://github.com/micronative/",
  "license": "MIT",
  "authors": [
    {
      "name": "Ken Ngo",
      "email": "ngodinhloc@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "psr/log": "1.0.2",
    "symfony/yaml": "^5.0",
    "symfony/http-foundation": "5.4.x-dev",
    "bramus/router": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Micronative\\MockServer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Samples\\": "samples/"
    }
  },
  "minimum-stability": "dev"
}
